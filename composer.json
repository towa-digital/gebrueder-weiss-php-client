{
    "name": "towa/gbw-sdk",
    "description": "API to create and retrieve logistics orders",
    "keywords": [
        "openapitools",
        "openapi-generator",
        "openapi",
        "php",
        "sdk",
        "rest",
        "api"
    ],
    "license": "GPL-3.0-only",
    "authors": [
        {
            "name": "Towa",
            "email": "developer@towa.at"
        }
    ],
    "require": {
        "php": ">=7.3",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^6.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0 || ^9.0",
        "friendsofphp/php-cs-fixer": "^2.12"
    },
    "autoload": {
        "psr-4": { "Towa\\GebruederWeissSDK\\" : "lib/" }
    },
    "autoload-dev": {
        "psr-4": { "Towa\\GebruederWeissSDK\\Test\\" : "test/" }
    }
}
